{
  "name": "@scheemer/framer-plugin",
  "private": true,
  "version": "1.0.0",
  "description": "Framer Plugin for exporting component metadata and structure as JSON",
  "type": "module",
  "author": "Scheemer Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nqh-public/scheemer.git",
    "directory": "framer-plugin"
  },
  "homepage": "https://github.com/nqh-public/scheemer#readme",
  "bugs": {
    "url": "https://github.com/nqh-public/scheemer/issues"
  },
  "keywords": [
    "framer",
    "framer-plugin",
    "component-export",
    "design-tools",
    "json-export",
    "metadata"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pack": "npx framer-plugin-tools@latest pack",
    "prepare": "husky"
  },
  "dependencies": {
    "framer-plugin": "^3",
    "react": "^19",
    "react-dom": "^19",
    "vite-plugin-mkcert": "^1"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "jsinspect": "^0.12.7",
    "lint-staged": "^16.2.6",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6",
    "vite-plugin-framer": "^1",
    "vitest": "^4.0.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
